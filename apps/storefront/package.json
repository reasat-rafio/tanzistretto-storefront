{
  "name": "storefront",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently 'vite dev' 'pnpm generate --watch'",
    "build": "shx rm -rf ./build && pnpm generate && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@parcel/watcher": "^2.4.1",
    "@portabletext/svelte": "^2.1.5",
    "@portabletext/types": "^2.0.8",
    "@sanity/client": "^6.14.3",
    "@sanity/eslint-config-studio": "^3.0.1",
    "@sanity/image-url": "^1.0.2",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "^8.56.0",
    "@types/react": "^18.2.58",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "graphql": "^16.8.1",
    "groq": "^3.30.0",
    "gsap": "^3.12.5",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-meta-tags": "^3.1.1",
    "sveltekit-search-params": "^2.1.2",
    "sveltekit-superfetch": "^3.0.3",
    "sveltekit-superforms": "^2.11.0",
    "sveltekit-turnstile": "^1.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@sanity/vision": "^3.30.0",
    "bits-ui": "^0.21.0",
    "embla-carousel-svelte": "^8.0.0",
    "lucide-svelte": "^0.363.0",
    "mode-watcher": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-is": "^18.2.0",
    "sanity": "^3.30.0",
    "svelte-sonner": "^0.3.19",
    "tailwind-variants": "^0.2.1",
    "vaul-svelte": "^0.3.0"
  }
}
